<Window x:Class="PrimerInteroperability.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Primer interop" Height="240" Width="320">
    <StackPanel Margin="5">
        <Button Content="Show a WinForm Modally" Click="OnDialogShow"/>
        <Button Content="Show a WinForm Modelessly" Click="OnModallyShow"/>
        <Button Content="Enable Modeless Support for WinForms" Click="OnModelessEnable"/>
        <Button Content="Host WinForms Control" Click="OnHostWinFormControl"/>
        <Button Content="Mnemonic Test (WPF)" Click="OnWpfMnemonicTest"/>
        <Button Content="Mnemonic Test (WinForms)" Click="OnWinFormsMnemonicTest"/>
    </StackPanel>
</Window>
