<Page x:Class="NavigationApplication.EmbeddedPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      WindowTitle="EmbeddedPage">
    <StackPanel Margin="5">
        <TextBlock Margin="5" TextWrapping="Wrap" Text="This page contains an embedded page in a frame." />
        <Frame Name="EmbeddedFrame" Margin="5" Source="Page1.xaml" BorderThickness="1" BorderBrush="Blue" />
        <CheckBox Name="OwnsJournalCheckBox" Margin="5" Click="OnCheckOwnsJournal"
                  Content="Embedded Frame Owns Journal" />
    </StackPanel>
</Page>