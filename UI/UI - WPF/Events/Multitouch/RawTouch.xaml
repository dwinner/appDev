<Window x:Class="Multitouch.RawTouch"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="RawTouch" Height="363" Width="568">
    <Grid>
        <Canvas
            x:Name="Canvas" Background="LightSkyBlue"
            TouchDown="Canvas_OnTouchDown" TouchUp="Canvas_OnTouchUp"
            TouchMove="Canvas_OnTouchMove" />
    </Grid>

</Window>