        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AssemblyResources" Height="300" Width="300">
    <StackPanel>
        <Button Click="OnPlay" Margin="5" Padding="5" Content="Play"/>
        <Image Name="Img" Margin="5" Source="Images/Blue hills.jpg" />
        <MediaElement Name="Sound" Source="Sounds/start.wav" LoadedBehavior="Manual" />
    </StackPanel>
</Window>