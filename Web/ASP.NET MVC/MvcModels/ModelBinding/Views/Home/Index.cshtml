@model ModelBinding.Models.Person

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title</h2>

<div>
    <label>Id:</label>
    @Html.DisplayFor(person => person.PersonId)
</div>
<div>
    <label>First name:</label>
    @Html.DisplayFor(person => person.FirstName)
</div>
<div>
    <label>Last name:</label>
    @Html.DisplayFor(person => person.LastName)
</div>
<div>
    <label>Role:</label>
    @Html.DisplayFor(person => person.Role)
</div>
<div>
    <label>City:</label>
    @Html.DisplayFor(person => person.HomeAddress.City)
</div>
<div>
    <label>Country:</label>
    @Html.DisplayFor(person => person.HomeAddress.Country)
</div>